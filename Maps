#include<bits/stdc++.h>
using namespace std;
int main()
{
	/*maps in cpp which contains keys and values
	two types of maps
    1.Ordered Map
    2.Unordered Map*/
    map<int,int>mpp;
    /*in cpp if a key is not present it insert 
    the key and it makes it value to zero*/
    int arr[10]={1,2,4,3,3,1,2,1,3,2};
    for(int i=0;i<10;i++)
    {
    	mpp[arr[i]]++;

    }
    for(auto it:mpp)
    {
    	cout<<it.first<<" "<<it.second<<"\n";
    }
    

}
